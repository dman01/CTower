<h1>Listing deliverables</h1>

<table>
  <tr>
    <th>Initiativeid</th>
    <th>Name</th>
    <th>Uniqueid</th>
    <th>Description</th>
    <th>Completionforecast</th>
    <th>Completiondegree</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @deliverables.each do |deliverable| %>
  <tr>
    <td><%= deliverable.initiativeId %></td>
    <td><%= deliverable.name %></td>
    <td><%= deliverable.uniqueID %></td>
    <td><%= deliverable.description %></td>
    <td><%= deliverable.completionForecast %></td>
    <td><%= deliverable.completionDegree %></td>
    <td><%= link_to 'Show', deliverable %></td>
    <td><%= link_to 'Edit', edit_deliverable_path(deliverable) %></td>
    <td><%= link_to 'Destroy', deliverable, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Deliverable', new_deliverable_path %>
